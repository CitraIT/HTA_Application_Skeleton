<HTA:APPLICATION 
APPLICATIONNAME = "HTA Application Template"
SINGLEINSTANCE = "yes"
ICON = "path/to/icon.ico" 
SCROLL = "no"
MAXIMIZEBUTTON = "no"
NAVIGABLE = "no"
SELECTION = "no"
CONTEXTMENU = "no"
BORDER = "thin"
INNERBORDER = "no"
></HTA:APPLICATION>

<SCRIPT FOR="window" EVENT="onload">

onerror=handleLaunchErr;
initialize();

</SCRIPT>

<SCRIPT>

// Event handler for window.onload()
function initialize()   {
    initWindow();
    document.body.style.visibility = "visible";
}

// Applies initial state settings for the application
function initWindow()   {
    // delay the call and avoid 'Access denied' exception
    setTimeout(setSize1, 0);
}

function setSize1(){
    setWindowSize(725, 585);
}

function setSize2(){
    setWindowSize(725, document.body.scrollHeight + 25);
}



// Opens a new window
function popWindow(url, name)
{
	var wndPopup = window.open(url,name,"");
	// Bring new window to foreground
	if(wndPopup) wndPopup.focus();
}

// Resizes the current window
function setWindowSize(width, height){
    window.resizeTo(width, height);
}


function launch(program){
(new ActiveXObject('WScript.Shell')).Run(program);void(0);
}

function handleLaunchErr(msg,url,l)
{
    alert(msg);
    return true;
}

// Immediate execution
//

</SCRIPT>
 
